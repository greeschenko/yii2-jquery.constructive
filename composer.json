{
    "name": "greeschenko/yii2-jquery.constructive",
    "description": "jquery full page effects set for yii2",
    "type": "yii2-extension",
    "keywords": ["yii2","extension","jquery.constructive","page effects"],
    "license": "MIT",
    "authors": [
        {
            "name": "Oleksiy Hryshchenko",
            "email": "greeschenko@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "yiisoft/yii2": "*",
        "bower-asset/jquery.constructive": "~0.0.12"
    },
    "autoload": {
        "psr-4": {
            "greeschenko\\jconstructive\\": ""
        }
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
